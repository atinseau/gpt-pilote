{
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "airbnb/hooks"
    ],
    "parserOptions": {
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
        "no-constant-condition": "off",
        "no-await-in-loop": "off",
        "no-console": "off",
        "react/function-component-definition": "off"
    }
}