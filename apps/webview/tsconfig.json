{
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2022",
		"outDir": "out",
		"moduleResolution": "nodenext",
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"lib": [
			"ES2020",
			"DOM",
			"DOM.Iterable"
		],
		"sourceMap": true,
		"baseUrl": ".",
		"strict": true
	},
	"include": [
		"app",
		".eslintrc.js"
	],
	"exclude": [
		"node_modules",
		"build"
	]
}
